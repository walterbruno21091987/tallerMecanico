<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android">
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="4dp"
        android:background="@color/black"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_fecha_emicion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="336dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_tit_fecha_reparacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:text="fecha reparacion:"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_tit_cod_reparacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="28dp"
        android:text="REPARACION NUMERO"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_fecha_reparacion" />

    <TextView
        android:id="@+id/tv_cod_reparacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="64dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_cod_reparacion"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_tit_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="CLIENTE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_cod_reparacion" />

    <TextView
        android:id="@+id/tv_cod_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_cliente"
        app:layout_constraintTop_toBottomOf="@+id/tv_cod_reparacion" />

    <TextView
        android:id="@+id/tv_tit_patente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="PATENTE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_cliente" />

    <TextView
        android:id="@+id/tv_patente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_patente"
        app:layout_constraintTop_toBottomOf="@+id/tv_cod_cliente" />

    <TextView
        android:id="@+id/tv_asegurado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="144dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_asegurado"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_tit_asegurado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="60dp"
        android:text="ASEGURADO"
        app:layout_constraintStart_toEndOf="@+id/tv_patente"
        app:layout_constraintTop_toBottomOf="@+id/tv_cod_reparacion" />

    <TextView
        android:id="@+id/tv_tit_repuestos"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="REPUESTOS"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_patente" />

    <TextView
        android:id="@+id/tv_repuesto"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_repuestos"
        app:layout_constraintTop_toBottomOf="@+id/tv_patente" />

    <TextView
        android:id="@+id/tv_tit_mano_de_obra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="20dp"
        android:text="MANO DE OBRA"
        app:layout_constraintStart_toEndOf="@+id/tv_repuesto"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_asegurado" />

    <TextView
        android:id="@+id/tv_mano_de_obra"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_mano_de_obra"
        app:layout_constraintTop_toBottomOf="@+id/tv_asegurado" />

    <TextView
        android:id="@+id/tv_fecha_reparacion"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="16dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_fecha_reparacion"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_tit_descuento_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="DESC. CLIENTE"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_repuestos" />

    <TextView
        android:id="@+id/tv_descuento_cliente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_descuento_cliente"
        app:layout_constraintTop_toBottomOf="@+id/tv_repuesto" />
    <TextView
        android:id="@+id/tv_tit_descuento_seguro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="DESC. SEGURO"
        app:layout_constraintStart_toEndOf="@id/tv_descuento_cliente"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_mano_de_obra" />

    <TextView
        android:id="@+id/tv_descuento_seguro"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_descuento_seguro"
        app:layout_constraintTop_toBottomOf="@+id/tv_mano_de_obra" />

    <TextView
        android:id="@+id/tv_tit_total"
        android:layout_width="66dp"
        android:layout_height="30dp"
        android:layout_marginStart="156dp"
        android:layout_marginTop="32dp"
        android:text="TOTAL"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_descuento_cliente" />

    <TextView
        android:id="@+id/tv_total"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@+id/tv_tit_total"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_descuento_seguro" />

    <Button
        android:id="@+id/bt_ver_repuestos"
        android:layout_width="128dp"
        android:layout_height="56dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="20dp"
        android:text="ver repuestos"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_tit_descuento_cliente" />
</androidx.constraintlayout.widget.ConstraintLayout>
</layout>